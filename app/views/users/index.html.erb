<div class="container">
  <h2>ユーザー</h2>

  <% @users.each do |user| %>
    <% unless current_user.id == (user.id)%>
      <div class="container-child col-md-2 col-sm-4 col-xs-6">
        <%= link_to user_path(user) do %>
          <h4><strong><%= user.name %></strong></h4>
        <% end %>
        <%= profile_img(user) %>
        <%= render 'follow_form', user: user %>
      </div>
    <% end %>
  <% end %>
</div>
